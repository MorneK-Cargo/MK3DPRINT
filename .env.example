# mk3dprint.org Environment Variables Template
# Copy to .env and fill with actual values for development
# DO NOT commit actual .env to Git - use .env.example as template

# Database Connection (different per environment)
DATABASE_URL=postgresql://user:password@hostname:5432/database_name?connect_timeout=15

# AbacusAI API Key
ABACUSAI_API_KEY=your_abacus_api_key_here

# App Configuration
WEB_APP_ID=your_web_app_id_here

# Notification IDs
NOTIF_ID_CONTACT_FORM_SUBMISSION=your_notification_id_here
NOTIF_ID_QUOTE_REQUEST=your_notification_id_here

# Next.js Build Configuration
NEXT_OUTPUT_MODE=export
NODE_ENV=development

# Environment Name (for logging/tracking)
ENVIRONMENT=development

# Optional: API Endpoints (vary by environment)
# NEXT_PUBLIC_API_URL=http://localhost:3000
# NEXT_PUBLIC_LOG_LEVEL=debug
